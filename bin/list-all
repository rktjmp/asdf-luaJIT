#!/usr/bin/env bash

versions_list=(
  2.1.0-beta3--3.4.0
  2.1.0-beta2--3.4.0
  2.1.0-beta1--3.4.0
  2.0.5--2.4.4
  2.0.4--2.4.4
  2.0.3--2.4.4
  2.0.2--2.4.4
  2.0.1--2.4.4
  2.0.0--2.4.4
)

versions=""

for version in "${versions_list[@]}"
do
  versions="${versions} ${version}"
done

echo $versions
